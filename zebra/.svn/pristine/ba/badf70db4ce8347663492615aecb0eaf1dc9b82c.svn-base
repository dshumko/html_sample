
#container  {
    background-color:#F0F0F0;
    margin:20px 100px;
    width:1060px;
}

#runner, #lister {
    visibility:hidden;
    display:none;
}

.zone1_board {
    background-color: #4C5444;
    font-size: 130%;
    width:1060px;
    height: 40px;
    overflow:hidden;
    /*transition: all 1s;*/
}

.zone2_board {
    background-color: #C2C6BE;
    font-size: 150%;
    height:620px;
    float:left;
    overflow:hidden;
    /*transition: all 1s;*/
}

.zone3_board {
    background-color: #DDDDD7;
    font-size: 120%;
    height:620px;
    float:left;
    overflow:hidden;
    /*transition: all 1s;*/
}

.zone4_board {
    background-color:#EAEBD3;
    font-size: 90%;
    height:620px;
    float:left;
    overflow:hidden;
    /*transition: all 1s;*/
}
.zone2_p_width  {
    width:1000px;
}
.zone2_p_width .zone2_c_width {
    width:inherit;
}

.zone3_p_width {
    width:30px;
}
.zone3_p_width .zone3_c_width {
    width:inherit;
}

.zone4_p_width {
    width:30px;
}
.zone4_p_width .zone4_c_width {
    width:inherit;
}

.zone2_btn {
    background-color: #C2C6BE;
    font-size: 70%;
    height: 35px;
    float:left;
    /*transition: all 1s;*/
}

.zone3_btn {
    background-color: #DDDDD7;
    font-size: 80%;
    height:25px;
    float:left;
    /*transition: all 1s;*/
}

.zone4_btn {
    background-color:#EAEBD3;
    font-size: 90%;
    height:20px;
    float:left;
    /*transition: all 1s;*/
}

.width40 {
    width: 30px;
    opacity:0.6;
}

.width400 {
    width: 310px;
}

.width840 {
    width: 720px;
}

.width1200 {
    width: 1000px;
}

.logo {
    width: 90px;
    height: 40px;
    float:left;
    background-image: url("../../res/zebra_logo1.png");
    background-size: contain;
}

.status {
    float:right;
    font-size: 60%;
    color: white;
    height:10px;
    vertical-align:bottom;
}

.result_pass {
    color:green;
}

.result_fail {
    color:red;
}

.checked {
    background-color:#BCA979;
}

.focused {
    background-color: #CCB989;
}