
#container  {
    background-color:#F0F0F0;
    width:1260px;
}

#runner, #lister {
    visibility:hidden;
    display:none;
}

.zone1_board {
    background-color: #4C5444;
    font-size: 130%;
    width:1260px;
    height: 40px;
    overflow:hidden;
    /*transition: all 1s;*/
}

.zone2_board {
    background-color: #C2C6BE;
    font-size: 150%;
    height:660px;
    float:left;
    overflow:hidden;
    /*transition: all 1s;*/
}

.zone3_board {
    background-color: #DDDDD7;
    font-size: 120%;
    height:660px;
    float:left;
    overflow:hidden;
    /*transition: all 1s;*/
}

.zone4_board {
    background-color:#EAEBD3;
    font-size: 90%;
    height:660px;
    float:left;
    overflow:hidden;
    /*transition: all 1s;*/
}
.zone2_p_width  {
    width:1180px;
}
.zone2_p_width .zone2_c_width {
    width:inherit;
}

.zone3_p_width {
    width:40px;
}
.zone3_p_width .zone3_c_width {
    width:inherit;
}

.zone4_p_width {
    width:40px;
}
.zone4_p_width .zone4_c_width {
    width:inherit;
}

.zone1_btn {
    background-color: #4C5444;
    font-size: 130%;
    color: white;
    font-weight: bolder;
    text-align:center;
    width: 120px;
    height: 40px;
    float:left;
    /*transition: all 1s;*/
}

.zone2_btn {
    background-color: #C2C6BE;
    font-size: 110%;
    height: 35px;
    float:left;
    /*transition: all 1s;*/
}

.zone3_btn {
    background-color: #DDDDD7;
    font-size: 100%;
    height:32px;
    float:left;
    /*transition: all 1s;*/
}

.zone4_btn {
    background-color:#EAEBD3;
    font-size: 90%;
    height:20px;
    float:left;
    /*transition: all 1s;*/
}

.width40 {
    width: 40px;
    opacity:0.7;
}

.width400 {
    width: 400px;
}

.width840 {
    width: 820px;
}

.width1200 {
    width: 1180px;
}

.logo {
    width: 90px;
    height: 40px;
    float:left;
    background-image: url("../../res/zebra_logo1.png");
    background-size: contain;
}

.status {
    float:right;
    font-size: 60%;
    color: white;
    height:10px;
    vertical-align:bottom;
}

.result_pass {
    color:green;
}

.result_fail {
    color:red;
}

.checked {
    background-color:#BCA979;
}

.focused {
    background-color: #CCB989;
}